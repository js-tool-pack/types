{
  "name": "lib-starter",
  "version": "0.0.0",
  "packageManager": "pnpm@7.1.0",
  "description": "一个基于pnpm typescript monorepo/multirepo的lib快速启动仓库模板",
  "scripts": {
    "pkg:init": "esno scripts/init-pkg.ts",
    "pkg:new": "esno scripts/new-pkg.ts",
    "check-deps": "ncu",
    "test": "jest",
    "cov": "jest --coverage",
    "prepare": "husky install && npm run pkg:init",
    "husky-msg-init": "husky add .husky/pre-commit \"npx lint-staged --allow-empty \"$1\"\"&husky add .husky/commit-msg \"npx commitlint --edit \"$1\"\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ts-check": "tsc --noEmit && tsc -p ./tsconfig.node.json --noEmit",
    "lint-check": "eslint .",
    "prettier-check": "prettier ./ --check",
    "check-all": "run-p ts-check lint-check prettier-check",
    "typedoc:build": "typedoc --plugin @mxssfd/typedoc-theme --theme my-theme",
    "typedoc:deploy": "gh-pages -d docs-html",
    "build": "esno scripts/build.ts -t",
    "release": "esno scripts/release.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,html,md,yaml}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengxinssfd/lib-starter.git"
  },
  "keywords": [
    "lib",
    "starter"
  ],
  "author": "dyh_a",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mengxinssfd/lib-starter/issues"
  },
  "homepage": "https://github.com/mengxinssfd/lib-starter#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@microsoft/api-extractor": "^7.33.6",
    "@mxssfd/core": "5.0.1-beta.1",
    "@mxssfd/typedoc-theme": "^1.0.3",
    "@rollup/plugin-json": "^5.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.3",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.9",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "chalk": "^5.1.2",
    "conventional-changelog-cli": "^2.2.2",
    "core-js": "^3.26.1",
    "enquirer": "^2.3.6",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.3",
    "minimist": "^1.2.7",
    "npm-check-updates": "^16.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "sass": "^1.56.1",
    "semver": "^7.3.8",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE"
  ]
}
